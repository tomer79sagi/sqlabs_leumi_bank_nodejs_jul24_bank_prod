<h1>Transaction History</h1>
<% if (error) { %>
    <div class="flash error">
        <p><%= error %></p>
    </div>
<% } %>
<ul>
    <% if (transactions) { %>
        <% transactions.forEach(function(transaction) { %>
            <li><%= transaction.date %>: $<%= transaction.amount %> from <%= transaction.from %> to <%= transaction.to %></li>
        <% }); %>
    <% } %>
</ul>
<a href="/accounts">Back to Home</a>